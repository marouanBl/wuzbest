<h1>Listing shops</h1>

<table>
  <tr>
    <th>Main shop</th>
    <th>Web</th>
    <th>Contact phone</th>
    <th>Contact fax</th>
    <th>Contact email</th>
    <th>User</th>
    <th>Slug</th>
    <th>State</th>
    <th>City</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shops.each do |shop| %>
  <tr>
    <td><%= shop.main_shop_id %></td>
    <td><%= shop.web %></td>
    <td><%= shop.contact_phone %></td>
    <td><%= shop.contact_fax %></td>
    <td><%= shop.contact_email %></td>
    <td><%= shop.user_id %></td>
    <td><%= shop.slug %></td>
    <td><%= shop.state_id %></td>
    <td><%= shop.city_id %></td>
    <td><%= link_to 'Show', shop %></td>
    <td><%= link_to 'Edit', edit_shop_path(shop) %></td>
    <td><%= link_to 'Destroy', shop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Shop', new_shop_path %>
